<div class="card m-3">
  <img
    src="https://cdn.autobild.es/sites/navi.axelspringer.es/public/media/image/2018/12/lista-coches-nuevos-2019-lanzamientos.jpg"
    class="card-img-top" alt="...">
  <div class="card-body">
    <h5 class="card-title">
      <%= car.nombre %>
    </h5>
    <p class="card-text">
      <%= car.matricula %>
    </p>
    
  </div>
  <% if (!car.hora_sal) { %>
    <div class="card-footer tarjeta-abajo-dentro text-end">
      DENTRO
    </div>
  <% } else { %>
    <div class="card-footer tarjeta-abajo-fuera text-end">
      FUERA
    </div>
  <% } %>
</div>